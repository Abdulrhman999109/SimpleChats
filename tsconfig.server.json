{
    "compilerOptions": {
      "target": "ES2017",  
      "module": "esnext",  
      "moduleResolution": "Node",  
      "esModuleInterop": true,  
      "strict": true,
      "skipLibCheck": true,
      "noEmit": true,
      "allowJs": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true
    }
  }
  